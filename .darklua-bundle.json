{
  "bundle": {
    "require_mode": "path"
  },
  "process": [
    {
      "rule": "inject_global_value",
      "identifier": "LUA_ENV",
      "value": "roblox"
    },
    {
      "rule": "inject_global_value",
      "identifier": "__DEV__",
      "value": false
    },
    "remove_types",
    "remove_comments",
    "remove_spaces",
    "compute_expression",
    "remove_unused_if_branch",
    "remove_unused_while",
    "filter_after_early_return",
    "remove_nil_declaration",
    "remove_empty_do"
  ]
}
